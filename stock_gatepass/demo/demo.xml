<odoo>
    <data noupdate="0">
        <record id="seq_gate_pass" model="ir.sequence">
            <field name="name">Gate Pass</field>
            <field name="prefix">GTPS/IN/%(year)s/</field>
            <field name="implementation">no_gap</field>
            <field name="padding">5</field>
            <field name="number_increment">1</field>
        </record>
        <record id="seq_gate_pass_outward" model="ir.sequence">
            <field name="name">Gate Pass Outward</field>
            <field name="prefix">GTPS/OUT/%(year)s/</field>
            <field name="implementation">no_gap</field>
            <field name="padding">5</field>
            <field name="number_increment">1</field>
        </record>
        <record id="seq_inspection" model="ir.sequence">
            <field name="name">Inspection sequence</field>
            <field name="prefix">INSP/</field>
            <field name="padding">4</field>
            <field name="implementation">no_gap</field>
            <field name="number_increment">1</field>
        </record>
    </data>
</odoo>